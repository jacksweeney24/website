{
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main-domain/$1",
      "continue": true,
      "headers": {
        "host": "jacksweeney.xyz"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/sub-domain/$1",
      "continue": true,
      "headers": {
        "host": "services.jacksweeney.xyz"
      }
    }
  ]
}
